<h3>Language Detail</h3>

@if (CurrentLanguage is not null)
{
    <div class="detail-container">
        <p>Name : @CurrentLanguage.Name</p>
        <p>Level : @CurrentLanguage.Level</p>
    </div>
}
else
{
    <p>Loading Language details...</p>
}




































