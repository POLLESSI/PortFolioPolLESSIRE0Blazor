<h3>Interest Detail</h3>

@if (CurrentInterest is not null)
{
    <div class="detail-container">
        <p>Name : @CurrentInterest.Name</p>
        <p>Description : @CurrentInterest.Description</p>
    </div>
}
else
{
    <p>Loading Interest details...</p>
}


































